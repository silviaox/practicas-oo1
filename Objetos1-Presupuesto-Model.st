Object subclass: #Item	instanceVariableNames: 'detalle cantidad costoUnitario'	classVariableNames: ''	package: 'Objetos1-Presupuesto-Model'!!Item commentStamp: 'silviaOxalde 8/15/2020 22:21' prior: 0!Practica 1 OO1 - Ejercicio 3!!Item methodsFor: 'calculating' stamp: 'silviaOxalde 8/15/2020 22:27'!initializedetalle:='Texto indefinido'.cantidad:=0.costoUnitario:=0.! !!Item methodsFor: 'calculating' stamp: 'silviaOxalde 8/15/2020 22:28'!costoUnitario: unNumerocostoUnitario:=unNumero.! !!Item methodsFor: 'calculating' stamp: 'silviaOxalde 8/15/2020 22:27'!detalle: unTextodetalle:= unTexto! !!Item methodsFor: 'calculating' stamp: 'silviaOxalde 8/15/2020 22:26'!costo^(cantidad * costoUnitario).! !!Item methodsFor: 'calculating' stamp: 'silviaOxalde 8/15/2020 22:27'!cantidad: unNumerocantidad:= unNumero.! !!Item methodsFor: 'calculating' stamp: 'silviaOxalde 8/15/2020 22:36'!cantidad^ cantidad.! !!Item methodsFor: 'calculating' stamp: 'silviaOxalde 8/15/2020 22:37'!detalle^detalle.! !!Item methodsFor: 'calculating' stamp: 'silviaOxalde 8/15/2020 22:37'!costoUnitario^costoUnitario.! !Object subclass: #Presupuesto	instanceVariableNames: 'fecha cliente items'	classVariableNames: ''	package: 'Objetos1-Presupuesto-Model'!!Presupuesto methodsFor: 'calculating' stamp: 'silviaOxalde 8/15/2020 22:31'!agregarItem: unItemitems add: unItem.! !!Presupuesto methodsFor: 'calculating' stamp: 'silviaOxalde 8/15/2020 22:32'!calcularTotal|suma|suma:=0.items do:[:item | suma:= suma + item costo] .^suma.! !!Presupuesto methodsFor: 'calculating' stamp: 'silviaOxalde 8/15/2020 22:29'!initializefecha:= Date today.cliente:= 'Texto indefinido'.items:= OrderedCollection new.! !!Presupuesto methodsFor: 'calculating' stamp: 'silviaOxalde 8/15/2020 22:30'!cliente: unNombrecliente:= unNombre.! !!Presupuesto methodsFor: 'calculating' stamp: 'silviaOxalde 8/15/2020 22:39'!fecha: unaFechafecha:=unaFecha.! !!Presupuesto methodsFor: 'calculating' stamp: 'silviaOxalde 8/15/2020 22:39'!cliente^cliente.! !!Presupuesto methodsFor: 'calculating' stamp: 'silviaOxalde 8/15/2020 22:39'!fecha^fecha.! !